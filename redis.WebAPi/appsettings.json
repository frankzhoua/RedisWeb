{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Environment": "Fact",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "JwtSettings": {
    "SecretKey": "your_secret_key_for_jwt_with_at_least_256_bits_long",
    "Issuer": "MyAuthApi",
    "Audience": "MyAuthApiUsers",
    "ExpirationInMinutes": 180
  }
}
