{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=AuthDb;Integrated Security=True;"
  },
  "JwtSettings": {
    "SecretKey": "your_secret_key_for_jwt", //Replace with the actual key 
    "Issuer": "MyAuthApi",
    "Audience": "MyAuthApiUsers",
    "ExpirationInMinutes": 60
  }
}
